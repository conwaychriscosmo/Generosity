<div class="container-fluid" style="margin-top:-.2%" data-ng-init="profiledUser.getUserFromUrlParams()">
	<span class="profile-pic">
		<img src="http://cdn02.animenewsnetwork.com/thumbnails/max500x600/encyc/A15757-989316051.1386311773.jpg"></img>	
		<div>
			{{profiledUser.username}}
		</div>
	</span>
	<span class="profile-info">
		<span class="tag">Real name: </span>{{profiledUser.realName}}
		<br>
		<span class="tag">Best timeframe: </span>{{profiledUser.availableHours}}
		<br>
		<span class="tag">Current city: </span>{{profiledUser.currentCity}}
		<br>
		<span class="tag">Reputation: </span>{{profiledUser.reputation}}
		<br>
		<span class="tag">About me: </span>{{profiledUser.description}}
	</span>
</div>